package thu.qjp.preprocess

object Preprocess {
  def main (args: Array[String]) {
  	print(URLDecoder.decode("http%3A%2F%2Fent.sina.com.cn%2Fc%2F2009-01-13%2F01492340274.shtml%23Main"))
  }
  import java.io.File
  import java.net.URLDecoder.{decode => unquote}
  print(unquote("%2F"))
  def stripBlank(input: Array[String]) = {
    input.filter(_.length != 0).map(_.trim)
  }
  
}